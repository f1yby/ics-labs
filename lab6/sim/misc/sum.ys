.pos 0
init:
    irmovq stack, %rsp
    irmovq ele1, %rdi
    call sum_list
    halt
.align 8
ele1:
    .quad 0x00a
    .quad ele2
ele2:
    .quad 0x0b0
    .quad ele3
ele3:
    .quad 0xc00
    .quad 0

sum_list:
    xorq %rax, %rax
    jmp compare
loop:
    mrmovq (%rdi), %rbx
    addq  %rbx, %rax
    mrmovq $0x8(%rdi), %rdi
compare:
    andq %rdi, %rdi
    jne loop
    ret

.pos 300
stack:
